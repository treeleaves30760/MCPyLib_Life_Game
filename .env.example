# MCPyLib Server Configuration
# Copy this file to .env and fill in your actual values

# Minecraft Server IP (default: 127.0.0.1)
SERVER_IP=127.0.0.1

# Minecraft Server Port (default: 65535)
SERVER_PORT=65535

# MCPyLib Authentication Token
SERVER_TOKEN=your_token_here

# Player Name
PLAYER_NAME=your_player_name_here